{
  "name": "magicbook",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "npx dotenv-cli -e .env.test -- npx prisma migrate deploy && npx dotenv-cli -e .env.test -- cypress run --config-file cypress.config.js",
    "dev:test": "npx dotenv-cli -e .env.test -- next dev -p 3001",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@tailwindcss/postcss": "^4.1.14",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.14",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bluebird": "^3.5.42",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cypress": "^15.4.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1"
  },
  "prisma": {
    "seed": "node -r ts-node/register/transpile-only prisma/seed.ts"
  }
}
