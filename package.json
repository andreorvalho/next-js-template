{
  "name": "magicbook",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "npx dotenv-cli -e .env.test -- npx prisma migrate deploy && npx dotenv-cli -e .env.test -- cypress run",
    "dev:test": "npx dotenv-cli -e .env.test -- next dev -p 3001",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "@next/font": "^14.2.15",
    "@prisma/client": "^6.3.1",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "next": "^15.1.7",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bluebird": "^3.5.42",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cypress": "^14.0.3",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "wait-on": "^9.0.1"
  },
  "prisma": {
    "seed": "npx ts-node prisma/seed.ts"
  }
}
